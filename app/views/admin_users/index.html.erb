<p id="notice"><%= notice %></p>

<h1>Listing Admin Users</h1>

<table>
  <thead>
   <tr>
    <th>Users</th>
    <th>Action</th>
  </tr>
  </thead>

  <tbody>
    <% @admin_users.each do |admin_user| %>
      <tr>
        <td><%= admin_user.username %></td>
        <td><%= link_to 'Show', admin_user %>
        <%= link_to 'Edit', edit_admin_user_path(admin_user) %>
        <%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin user', new_admin_user_path %>
